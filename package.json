  "homepage": "https://github.com/henrikjoreteg/internal-nav-helper",
  "keywords": [
    "navigation",
    "preact",
    "react",
    "redux-bundler",
    "router",
    "routing",
    "spa"
  ],
  "license": "MIT",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "repository": {
